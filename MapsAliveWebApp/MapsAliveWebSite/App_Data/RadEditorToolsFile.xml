	<snippets>
		<snippet name="image-macro">
			<![CDATA[
				[image-macro hotspot-id:hotspotId; map-id:mapId; width:imageWidth; height:imageHeight]
			]]>
		</snippet>
		<snippet name="linkto-macro">
			<![CDATA[
					[linkto-macro map-id:mapId; hotspot-id:hotspotId; link-text:linkText; tooltip:tooltipText]
			]]>
		</snippet>
		<snippet name="flash-macro">
			<![CDATA[
					[flash-macro src:URL; width:videoWidth; height:videoHeight] 
			]]>
		</snippet>
		<snippet name="iframe-macro">
			<![CDATA[
					[iframe-macro src:URL; width:iframeWidth; height:iframeHeight]
			]]>
		</snippet>
		<snippet name="tour-macro">
			<![CDATA[
					[tour-macro tour:tourNumber; src:URL]
			]]>
		</snippet>
	</snippets>

